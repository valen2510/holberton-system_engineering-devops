[Unit]
Description=Gunicorn instance to serve 2-hbnb
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/AirBnB_clone_v4/
ExecStart=/home/ubuntu/.local/bin/gunicorn --workers 3 --bind http://127.0.0.1:5003 web_dynamic.2-hbnb:app --error-logfile /tmp/airbnb-error.log --access-logfile /tmp/airbnb-access.log

[Install]
WantedBy=multi-user.target
